int test (int n) {
    if (n > 0) {
	return test (n - 1);
    }
    return 10;
}

void generateTab (int n) {
    let tab = [];
    for (i in [0 .. n]) {
	tab = tab + i;
    }
    return tab;
}

int facto (int a) {
    let res = 1;
    for (i in [1 .. a]) {
        res = res * i;
    }
    return res;
}

int factoRec (int a) {
    if (a == 0) {
	return 1;
    }
    return a * factoRec (a - 1);
}

